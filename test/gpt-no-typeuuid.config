image __IMAGE__ {
	hdimage {
		align = 32K
		partition-table-type = gpt
	}

	partition oldstate {
		image = /dev/zero
		in-partition-table = false
		offset = 32K
		size = 32K
	}

	partition part1 {
		image = /dev/zero
		size = 64K
	}

	partition part2 {
		image = /dev/zero
		size = 32K
		partition-uuid = __GPT_LOOPDEV_PARTUUID__
	}

	partition oldstate2 {
		image = /dev/zero
		in-partition-table = false
		offset = 160K
		size = 32K
	}
}

